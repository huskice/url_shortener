generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Url {
  id          Int           @id @default(autoincrement())
  originalUrl String
  code        String        @unique
  urlAnalytic UrlAnalytic[]
}

model UrlAnalytic {
  id      Int @id @default(autoincrement())
  url     Url @relation(fields: [url_id], references: [id])
  url_id  Int @unique
  browserName String?
  countryName String?
  cookie String?
  referer String?
}
